<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mfproductlabel>
            <general>
                <enabled>0</enabled>
                <mfmodule>ProductLabel</mfmodule>
                <mftype>1</mftype>
                <labels_per_product>1</labels_per_product>
                <pp_selector>.fotorama__stage__shaft</pp_selector>
                <pl_selector>.product-image-wrapper</pl_selector>
                <exclude_page_types></exclude_page_types>
                <custom_positions>
                    {
                        "_1736868521129_129":{"position":"product_list_before_name"},
                        "_1736868522601_601":{"position":"product_list_after_price"},
                        "_1736868553357_357":{"position":"product_page_before_price"},
                        "_1736868660176_176":{"position":"product_page_before_reviews"}
                    }
                </custom_positions>
                <rule_validation_scope>0</rule_validation_scope>
            </general>
        </mfproductlabel>

        <system>
            <media_storage_configuration>
                <allowed_resources>
                    <media_gallery_image_folders>
                        <magefan_tc>magefan_product_label</magefan_tc>
                    </media_gallery_image_folders>
                </allowed_resources>
            </media_storage_configuration>
        </system>

    </default>
</config>
